public class MyClassDrive {
    public static void main(String[] args) {
        MyClass degisken = new MyClass();
        System.out.println("isim"+" " +degisken.isim);
        System.out.println("soyisim"+" "+degisken.soyisim);
        System.out.println("yas"+" "+degisken.yas);
        System.out.println("uni"+" "+degisken.uni);
        System.out.println("bolum"+" "+degisken.bolum);
        System.out.println("sac"+" "+degisken.sac);
        System.out.println("boy"+" "+degisken.boy);
        System.out.println("biyik"+" "+degisken.biyik);
        System.out.println("sakal"+" "+degisken.sakal);
        System.out.println("hobby"+" "+degisken.hobby);

